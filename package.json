{
    "name": "AppCongChung",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "link": "npx react-native link",
        "android": "npx react-native run-android",
        "ios": "npx react-native run-ios",
        "start": "npx react-native start",
        "format": "npm-run-all format:*",
        "format:js": "prettier --write {.,**}/*.js",
        "format:json": "prettier --write {.,**}/*.json",
        "format:md": "prettier --write {.,**}/*.md",
        "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
        "test": "jest",
        "pod": "cd ios && pod install && cd ..",
        "codePushAndroid": "code-push release-react CongChung-Android android -d Production",
        "codePushIOS": "code-push release-react CongChung-IOS ios -d Production",
        "apk-debug": "cd android && ./gradlew assembleDebug && cd ../",
        "apk-release": "cd android && ./gradlew assembleRelease && cd ../",
        "apk-install-debug": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
        "apk-install-release": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
        "aab": "./gradlew bundleRelease",
        "lint": "eslint index.js app --fix --ext .js,.ts,.tsx",
        "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081"
    },
